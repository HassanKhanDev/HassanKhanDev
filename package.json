{
  "name": "hiresync-hrms",
  "version": "1.0.0",
  "description": "HireSync - AI-Powered Human Resource Management SaaS Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-deps": "cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "setup": "chmod +x setup.sh && ./setup.sh"
  },
  "keywords": [
    "hrms",
    "hr",
    "human-resources",
    "saas",
    "mern",
    "ai",
    "recruitment",
    "payroll",
    "attendance"
  ],
  "author": "HireSync Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/hiresync.git"
  },
